    "bcrypt-nodejs": "^0.0.3",
    "body-parser": "^1.17.2",
    "dotenv": "^6.0.0",
    "express": "^4.15.4",
    "i": "^0.3.6",
    "if-env": "^1.0.4",
    "jquery": "^3.3.1",
    "jsonwebtoken": "^8.3.0",
    "mongoose": "^5.2.17",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "react-materialize": "^2.4.6",
